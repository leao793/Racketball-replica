      "image-gallery#home", //componente REACT galeria
      "footer-layout.desktop"
    ]
  },

// -----Hero-------

"flex-layout.row#hero-with-banners": {
    "children": [
      "flex-layout.col#slider-col",
      "flex-layout.col#banner-col"
    ],
    "props": {
      "colGap": 2,
      "rowGap": 0,
      "marginBottom": 6,
      "fullWidth": true
    }
  },

  // Columna Slider
  "flex-layout.col#slider-col": {
    "children": ["slider-layout#hero"],
    "props": {
      "width": "75%" 
    }
  },

  // Columna banners
  "flex-layout.col#banner-col": {
    "children": [
      "image#banner-for-her",
      "image#banner-for-him"
    ],
    "props": {
      "width": "25%",
      "rowGap": 1
    }
  },

  // ----- Slider principal -----
  "slider-layout#hero": {
    "props": {
      "itemsPerPage": { "desktop": 1, "tablet": 1, "phone": 1 },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "showPaginationDots": "always"
    },
    "children": [
      "image#hero1",
      "image#hero2",
      "image#hero3",
      "image#hero4",
      "image#hero5"
    ]
  },
  "image#hero1": {
    "props": {
