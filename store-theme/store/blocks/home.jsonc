{
  "store.home": {
    "blocks": [
      "header-layout.desktop",
      "flex-layout.row#hero-with-banners", // hero
      "rich-text#categories-title", // Título para la cuadrícula
      "flex-layout.row#home-categories", // Cuadrícula de Categorías
      "rich-text#esenciales-title", // Título para esenciales
      "list-context.product-list#esenciales", // Vitrina "esenciales"
      "rich-text#nuevacolección-title", // Título para nuevos productos
